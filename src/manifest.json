{
    "author": "Federico Jose Lucia",
    "developer": {
      "name": "Federico Jose Lucia",
      "url": "https://github.com/FedericoJoseLucia"
    },

    "manifest_version": 2,
    "name": "DuckDuckGo TryGoogle",
    "version": "2.0",
  
    "description": "Try Google allows you to retry your DuckDuckGo search with the google search engine",
  
    "icons": {
      "48": "assets/dax-logo.svg",
      "96": "assets/dax-logo.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.duckduckgo.com/*"],
        "js": ["assets/jquery.js", "tryGoogle.js"]
      }
    ]
}
